  "dependencies": {
    "@openai/agents": "^0.4.4",
    "axios": "^1.7.9",
    "openai": "^4.79.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
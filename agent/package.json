{
  "name": "@emergency-orchestrator/agent",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node ./run-worker.js",
    "dev": "node --watch ./run-worker.js",
    "lint": "tsc --noEmit",
